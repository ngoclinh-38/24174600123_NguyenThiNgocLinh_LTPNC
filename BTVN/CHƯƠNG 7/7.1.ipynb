   "source": [
    "import tkinter as tk\n",
    "\n",
    "# Tạo cửa sổ chính\n",
    "window = tk.Tk()\n",
    "\n",
    "# Đặt tiêu đề cho cửa sổ\n",
    "window.title(\"Hello Tkinter\")\n",
    "\n",
    "# Giữ cửa sổ hiển thị\n",
    "window.mainloop()\n"
   ]
  }
